{
  "name": "nounseek-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn run dev:css\" \"next dev\"",
    "dev:css": "tailwindcss -w -i ./styles/input.css -o styles/globals.css",
    "build": "npm run build:css && next build",
    "build:css": "tailwindcss -m -i ./styles/input.css -o styles/globals.css",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@nouns/assets": "^0.4.0",
    "@nouns/sdk": "^0.2.0",
    "@rainbow-me/rainbowkit": "^0.7.2",
    "classnames": "^2.3.2",
    "ethers": "^5.7.1",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "wagmi": "^0.8.5"
  },
  "devDependencies": {
    "@types/react": "18.0.21",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.4.0",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1",
    "typescript": "4.8.4"
  }
}
